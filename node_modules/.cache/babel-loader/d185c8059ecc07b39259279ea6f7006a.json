iv>\n</template>\n\n<script>\nimport vHead from \"./Header.vue\";\nimport vSidebar from \"./Sidebar.vue\";\nimport vTags from \"./Tags.vue\";\nimport bus from \"./bus\";\nimport { mapGetters } from \"vuex\";\nimport { mapActions } from \"vuex\";\nexport default {\n  data() {\n    return {\n      tagsList: [],\n      collapse: false,\n      form: {\n          name: '',\n          region: '',\n          date1: '',\n          date2: '',\n          delivery: false,\n          type: [],\n          resource: '',\n          desc: ''\n        },\n        formLabelWidth: '120px'\n    };\n  },\n  computed: {\n    ...mapGetters(\"dailog\", {\n      isShow: \"isShow\"\n    })\n  },\n  methods: {\n    ...mapActions(\"dailog\", [\"hideDailog\", \"showDailog\"]),\n    handleClose(){\n       this.$confirm('确认关闭？')\n          .then(_ => {\n            this.hideDailog();\n            //this.$store.dispatch('dailog/hideDailog')\n          }).bind(this)\n          .catch(_ => {});\n    }\n  },\n  components: {\n    vHead,\n    vSidebar,\n    vTags\n  },\n  created() {\n    bus.$on(\"collapse\", msg => {\n      this.collapse = msg;\n    });\n\n    // 只有在标签页列表里的页面才使用keep-alive，即关闭标签之后就不保存到内存中了。\n    bus.$on(\"tags\", msg => {\n      let arr = [];\n      for (let i = 0, len = msg.length; i < len; i++) {\n        msg[i].name && arr.push(msg[i].name);\n      }\n      this.tagsList = arr;\n    });\n  }\n};\n</script>\n"],"sourceRoot":"src\\components\\common"}]}